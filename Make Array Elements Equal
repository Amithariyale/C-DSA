// User function Template for C++

class Solution {
  public:
    long long int minOperations(int N) {
        // Code here
        
        long long int ans=0;
        
        if(N%2==0){
            ans=(N/2)*1ll*(N/2);
        }
        else{
            ans=((N-1)/2)*1ll*((N+1)/2);
        }
        
        return ans;
    }
};
