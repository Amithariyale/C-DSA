//User function Template for C++


class Solution
{

public:
    int xmod11(string x)
    {
       // code here
       
       int num=0;
       
       for(int i=0;i<x.size();i++){
           
           num=(num*10+x[i]-'0')%11;
           
           
       }
     
       return num%11;
    
    }
};
