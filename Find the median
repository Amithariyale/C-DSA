class Solution
{
public:
	public:
		int find_median(vector<int> v)
		{
		    sort(v.begin(),v.end());
		    int median=0;
		   if(v.size()%2==0){
		       
		       int i= (0+v.size()-1)/2;
		       median = (v[i]+v[i+1])/2;
		       return median;
		   }
		   else{
		       int i=v.size()/2;
		       return median=v[i];
		   }
		    
		}
};
