//User function Template for C++

class Solution{
public:
    void Reverse(stack<int> &St){
     
     int n=St.size();
     int* arr=new int[n];
     
     for(int i=0;i<n;i++){
         arr[i]=St.top();
         St.pop();
     }
     for(int i=0;i<n;i++){
         St.push(arr[i]);
     }
    }
};
